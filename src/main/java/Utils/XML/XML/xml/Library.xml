<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE library SYSTEM "library.dtd">
<libraries>
	<!-- 该图书馆的基本信息：图书馆的名字、地点、容量、开放时间等 -->
	<library name="" position="" capability="" opentime="">
		<!-- 书目基本信息（书籍名称、单价、ISBN、书目类别、作者、书籍编号）         入库信息（入馆时间、入馆数量、经办人） -->
		<book id="1">
			<basicInfo>
				<Name></Name>
				<Price></Price>
				<ISBN></ISBN>
				<Type></Type>
				<Author></Author>
				<ID></ID>
			</basicInfo>
			<extendInfo>
				<StorageTime></StorageTime>
				<StorageCount></StorageCount>
				<Agent></Agent>
			</extendInfo>
			<Press>
				<!-- 出版社基本信息（名称、地点、标志、联系方式等） -->
				<BasicInfo>
					<name></name>
					<position></position>
					<symbol></symbol>
					<!-- 联系方式包括：电话、网址、传真、email等联系方式 -->
					<contact>
						<MobilePhone></MobilePhone>
						<Fax></Fax>
						<Email></Email>
					</contact>
				</BasicInfo>
				<!-- 出版社主要出版图书信息（书目类型、出版方向，丛书名称） -->
				<AdvanceInfo>
					<Books></Books>
					<publish></publish>
					<publishbooks></publishbooks>
				</AdvanceInfo>
			</Press>
		</book>
		<!-- 出版社 -->
		
		<!-- 管理员信息（编号、姓名、联系方式、管理的范围） -->
		<Administrator>
			<identifier></identifier>
			<name></name>
			<contact></contact>
			<ControlRange></ControlRange>
		</Administrator>
	</library>
</libraries>